{
	// Place your snippets for dot here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

	"gen": {
		"prefix": "gen",
		"body": "arrowhead=empty"
	},
	"record": {
		"prefix": "rec",
		"body": "shape=record"
	},
	"circle": {
		"prefix": "cir",
		"body": "shape=circle"
	},
	"double circle": {
		"prefix": "dcir",
		"body": "shape=doublecircle"
	},
	"rankder=bt": {
		"prefix": "rankbt",
		"body": "rankdir=BT"
	},
	"dash": {
		"prefix": "dash",
		"body": "style=dashed"
	},
	"aggr": {
		"prefix": "aggr",
		"body": "arrowhead=ediamond"
	},
	"compose": {
		"prefix": "comp",
		"body": "arrowhead=diamond"
	},
	"label": {
		"prefix": "l",
		"body": "label=\"$1\""
	},
	"ellipse": {
		"prefix": "ell",
		"body": "shape=ellipse"
	},
	"box": {
		"prefix": "lab",
		"body": "[ label=\"$1\" ]"
	},
	"usecase": {
		"prefix": "usecase",
		"body": [
			"digraph usecase {",
			"  rankdir=LR",
			"  peripheries=0;",
			"",
			"  $1",
			"}"
		]
	},
	"actor": {
		"prefix": "act",
		"body": [
			"subgraph cluster_$1 {",
			"  label=\"$2\";",
			"  labelloc=b;",
			"  shape=none;",
			"  color=transparent;",
			"  $1",
			"};",
			"$1 [ label=\"\", shape=none, image=\"stick.png\" ]",
			"",
			"$0"
		]
	},
	"schema": {
		"prefix": "sch",
		"body": "arrowtail=vee, label=\"<<$1>>\", style=dashed"
	},
	"line": {
		"prefix": "lin",
		"body": "arrowhead=none"
	}
}