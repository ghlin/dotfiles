  "editor.tokenColorCustomizations": {
    "[Base16 Grayscale Light]": {
      "textMateRules": [
        {
          "name": "Constants",
          "scope": "constant",
          "settings": {
            "foreground": "#464646",
            "fontStyle": ""
          }
        },
        {
          "name": "Integers",
          "scope": "constant.numeric",
          "settings": {
            "foreground": "#464646",
            "fontStyle": ""
          }
        },
        {
          "name": "Floats",
          "scope": "none",
          "settings": {
            "foreground": "#464646",
            "fontStyle": ""
          }
        },
        {
          "name": "Boolean",
          "scope": "none",
          "settings": {
            "foreground": "#464646",
            "fontStyle": ""
          }
        },
        {
          "name": "Classes",
          "scope": "support.class, entity.name.class, entity.name.type.class",
          "settings": {
            "foreground": "#000000",
            "fontStyle": ""
          }
        },
        {
          "name": "Punctuation",
          "scope": "punctuation.definition.string, punctuation.definition.variable, punctuation.definition.string, punctuation.definition.parameters, punctuation.definition.string, punctuation.definition.array",
          "settings": {
            "foreground": "#464646"
          }
        },
        {
          "name": "Delimiters",
          "scope": "none",
          "settings": {
            "foreground": "#464646"
          }
        },
        {
          "name": "Keywords",
          "scope": "keyword",
          "settings": {
            "foreground": "#747474",
            "fontStyle": "underline"
          }
        },
        {
          "name": "Operators",
          "scope": "keyword.operator",
          "settings": {
            "foreground": "#464646",
            "fontStyle": ""
          }
        }
      ],
      "functions": {
        "fontStyle": "",
        "foreground": "#000000"
      },
      "variables": "#000000",
      "keywords": {
        "fontStyle": "underline",
        "foreground": "#000000"
      },
      "comments": {
        // "fontStyle": "italic",
        "foreground": "#979797"
      },
      "types": {
        "foreground": "#000000"
        // "fontStyle": ""
      }
    }
  },
  "workbench.activityBar.visible": false,
  "window.enableMenuBarMnemonics": false,
  "window.menuBarVisibility": "hidden",
  "vim.handleKeys": { "<C-f>": false, "<C-n>": true, "<C-p>": true },
  "vim.insertModeKeyBindings": [ { "before": [ "z", "z" ], "after": [ "<Esc>" ] } ],
  "vim.cursorStylePerMode.visual": "block",
  "vim.cursorStylePerMode.normal": "block",
  "editor.fontFamily": "'Lucida Console', 'Courier New', Consolas, 'Fira Code', 'SF Mono', Inconsolata, 'Fixedsys Excelsior', Monaco, Iosevka, monospace, 'Operator Mono Lig'",
  // "editor.fontWeight": "400",
  "workbench.editor.showTabs": false,
  "editor.tabSize": 2,
  "editor.detectIndentation": true,
  "editor.minimap.enabled": false,
  "terminal.integrated.shell.windows": "C:\\Windows\\System32\\cmd.exe",
  "haskell.hlint.run": "onSave",
  "terminal.explorerKind": "external",
  "editor.cursorStyle": "block",
  "editor.cursorWidth": 3,
  "window.title": "${dirty}${activeEditorShort} :: ${rootName}",
  "editor.wordSeparators": "`~!@%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.cursorBlinking": "blink",
  "git.autofetch": true,
  "files.eol": "\n",
  "editor.fontLigatures": true,
  "editor.renderLineHighlight": "none",
  "editor.overviewRulerBorder": false,
  "extensions.ignoreRecommendations": false,
  "editor.lineNumbers": "off",
  "advancedNewFile.exclude": { "node_modules": true },
  "tslint.autoFixOnSave": true,
  "editor.accessibilitySupport": "off",
  "workbench.statusBar.feedback.visible": false,
  /*
    https://stackoverflow.com/questions/40891692/any-way-to-disable-or-hide-visual-studio-codes-scrollbar-minimap
  */
  // "editor.scrollbar.horizontal": "hidden",
  // "editor.scrollbar.vertical": "hidden",
  "editor.renderIndentGuides": false,
  "editor.renderControlCharacters": true,
  "workbench.editor.showIcons": false,
  "workbench.tree.horizontalScrolling": true,
  "http.proxy": "http://localhost:8123",
  "editor.selectionHighlight": false,
  "indenticator.inner.showHighlight": false,
  "editor.roundedSelection": false,
  "editor.showFoldingControls": "always",
  "indenticator.inner.style": "inset",
  "indenticator.style": "inset",
  "liveServer.settings.donotShowInfoMsg": true,
  "zenMode.fullScreen": false,
  "todohighlight.isEnable": true,
  "todohighlight.defaultStyle": {
    "backgroundColor": "#000000",
    "color": "#FF0000"
  },
  "todohighlight.keywords": [ "NOTE", "XXX" ],
  "terminal.integrated.rendererType": "dom",
  "workbench.statusBar.visible": false,
  "workbench.commandPalette.preserveInput": true,
  "editor.wrappingIndent": "same",
  "workbench.iconTheme": null,
  "window.zoomLevel": 1,
  "editor.fontSize": 16,
  "vim.foldfix": true,
  "terminal.integrated.fontSize": 18,
  "editor.suggest.localityBonus": true,
  "editor.tabCompletion": "on",
  "files.watcherExclude": {
    "**/.buildd": true
  },
  "files.exclude": {
    "**/dist": true,
    "**/node_modules": true
  },
  "workbench.colorTheme": "Default High Contrast",
  "vim.cursorStylePerMode.insert": "line",
}
