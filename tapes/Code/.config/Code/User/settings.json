  "editor.tokenColorCustomizations": {
    "[Base16 Grayscale Light]": {
      "textMateRules": [
        {
          "name": "Constants",
          "scope": "constant",
          "settings": {
            "foreground": "#464646",
            "fontStyle": ""
          }
        },
        {
          "name": "Integers",
          "scope": "constant.numeric",
          "settings": {
            "foreground": "#464646",
            "fontStyle": ""
          }
        },
        {
          "name": "Floats",
          "scope": "none",
          "settings": {
            "foreground": "#464646",
            "fontStyle": ""
          }
        },
        {
          "name": "Boolean",
          "scope": "none",
          "settings": {
            "foreground": "#464646",
            "fontStyle": ""
          }
        },
        {
          "name": "Classes",
          "scope": "support.class, entity.name.class, entity.name.type.class",
          "settings": {
            "foreground": "#000000",
            "fontStyle": ""
          }
        },
        {
          "name": "Punctuation",
          "scope": "punctuation.definition.string, punctuation.definition.variable, punctuation.definition.string, punctuation.definition.parameters, punctuation.definition.string, punctuation.definition.array",
          "settings": {
            "foreground": "#464646"
          }
        },
        {
          "name": "Delimiters",
          "scope": "none",
          "settings": {
            "foreground": "#464646"
          }
        },
        {
          "name": "Keywords",
          "scope": "keyword",
          "settings": {
            "foreground": "#747474",
            "fontStyle": "underline"
          }
        },
        {
          "name": "Operators",
          "scope": "keyword.operator",
          "settings": {
            "foreground": "#464646",
            "fontStyle": "bold"
          }
        }
      ],
      "functions": {
        "fontStyle": "",
        "foreground": "#000000"
      },
      "variables": "#000000",
      "comments": {
        "fontStyle": "italic",
        "foreground": "#979797"
      },
      "types": {
        "foreground": "#000000",
        "fontStyle": ""
      }
    }
  },
